{
    "name": "aurelzefi/locale",
    "description": "A package to easily add localization in your Laravel project.",
    "license": "MIT",
    "authors": [
        {
            "name": "Aurel Zefi",
            "email": "aurelzefi1994@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aurel\\Locale\\LocaleServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Aurel\\Locale\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
